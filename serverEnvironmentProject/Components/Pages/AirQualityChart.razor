@page "/AirQualityChart"
@inject IJSRuntime JsRuntime
@rendermode InteractiveServer

<div id="AirQuality_chart_div" style="width: 400px; height: 120px;"></div>

@code {
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await JsRuntime.InvokeVoidAsync("drawAirQualityChart", "Air Quality", 80, "AirQuality_chart_div");
        }
    }
}
